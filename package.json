{
  "name": "contacthub-sdk-nodejs",
  "version": "0.1.0",
  "description": "ContactHub SDK for Node.js",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint src && eslint test",
    "flow": "flow",
    "test": "jest",
    "test-watch": "jest --watch",
    "e2e": "jest --config e2e.jest",
    "e2e-watch": "jest --watch --config e2e.jest",
    "clean": "rm -rf lib/*",
    "build": "npm run clean && babel src -d lib && flow-copy-source src lib",
    "prepare": "npm i --only=dev --ignore-scripts && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contactlab/contacthub-sdk-node"
  },
  "keywords": [
    "contacthub",
    "contactlab",
    "sdk",
    "node",
    "nodejs"
  ],
  "author": "Francesco Negri <francesco.negri@buildo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contactlab/contacthub-sdk-node/issues"
  },
  "homepage": "https://github.com/contactlab/contacthub-sdk-node#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^15.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.1.7",
    "eslint": "^3.6.1",
    "eslint-config-buildo": "github:buildo/eslint-config",
    "eslint-plugin-flowtype": "^2.19.0",
    "flow-bin": "^0.38.0",
    "flow-copy-source": "^1.1.0",
    "jest": "^18.1.0",
    "nock": "^8.0.0"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "postinstall-build": "^2.1.3",
    "uuid": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
